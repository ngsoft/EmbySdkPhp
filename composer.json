{
  "name": "ngsoft/emby-client-php",
  "description": "A php port of EmbyClient.JavaScript.",
  "keywords": [
    "emby",
    "sdk",
    "client",
    "rest",
    "api"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Aymeric Anger",
      "role": "Developer",
      "homepage": "https://github.com/ngsoft"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1",
    "ext-mbstring": "*",
    "ngsoft/tools": "^3.1",
    "psr/cache": "^2.0|^3.0",
    "psr/http-client": "^1.0",
    "tuupola/http-factory": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "EmbyClient\\": "src/",
      "EmbyClient\\Model\\": "lib/"
    },
    "files": []
  },
  "require-dev": {
    "brick/varexporter": "^0.4.0",
    "friendsofphp/php-cs-fixer": "^3.34",
    "guzzlehttp/guzzle": "^7.8",
    "ngsoft/cache": "^2.0",
    "symfony/cache": "^6.3",
    "symfony/var-dumper": "^6.3"
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  },
  "scripts": {
    "fix": "php vendor/bin/php-cs-fixer fix lib",
    "generate": "php src/generator.php -f"
  }
}
